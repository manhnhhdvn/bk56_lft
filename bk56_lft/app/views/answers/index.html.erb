<h1>Listing answers</h1>

<table >
  <thead>
    <tr>
      <th class="col-md-4">Question</th>
      <th>Title</th>
      <th >Description</th>
      <th ></th>
    </tr>
  </thead>

  <tbody>
    <% @answers.each do |answer| %>
      <tr>
        <td ><%= answer.id %></td>
        <td><%= answer.title %></td>
        <td><%= answer.description %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>













